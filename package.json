{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/mustache.yii/issues",
  "description": "Mustache templating for Yii Framework.",
  "homepage": "https://packagist.org/packages/cedx/mustache-yii",
  "license": "MIT",
  "name": "mustache-yii",
  "private": true,
  "version": "0.1.0",
  "devDependencies": {
    "archiver": "^0.11.0",
    "docgen-js": "^0.1.0",
    "jshint": "^2.5.2",
    "shelljs": "^0.3.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": true,
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "scripts": {
    "make": "node bin/make.js"
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/mustache.yii/favicon.ico",
    "name": "Mustache.yii",
    "url": "https://packagist.org/packages/cedx/mustache-yii",
    "options": {
      "extension": ".php",
      "outdir": "doc/api",
      "paths": "lib",
      "tabtospace": 2,
      "theme": "spacelab"
    }
  }
}
